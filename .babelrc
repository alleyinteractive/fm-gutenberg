{
  "plugins": [
    "transform-class-properties",
    [
      "module-resolver",
      {
        "extensions": [".js", ".jsx"],
        "root": ["./"]
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 2 versions", "ie >= 11"]
        }
      }
    ],
    "@babel/preset-react"
  ]
}
